<odoo>
    <template id="contact_proposal" name="contact_proposal">
        <t t-call="website.layout">
	    <t t-set="title">Contact a Proposal</t>
            <div class="container my-5">
                <h4 style="text-align: left;">
                    <font class="text-o-color-2">Contact a Research Proposal</font>
                    <br/>
                </h4>
                <section class="s_website_form" data-vcss="001" data-snippet="s_website_form" data-name="Form">
                    <form id="contactus_form" action="/website/form/" method="post" enctype="multipart/form-data" class="o_mark_required" data-mark="*" data-model_name="crm.lead" data-success-mode="redirect" data-success-page="/contactus-thank-you" data-pre-fill="true">
                        <div class="form-group s_website_form_field col-12 s_website_form_required" data-type="char" data-name="Field">
                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="o3oqfbw8kjq6">
                                    <span class="s_website_form_label_content">Your Name</span>
                                    <span class="s_website_form_mark"> *</span>
                                </label>
                                <div class="col-sm">
                                    <input type="text" class="form-control s_website_form_input" name="contact_name" required="1" data-fill-with="name" id="o3oqfbw8kjq6"/>
                                </div>
                            </div>
                        </div>
                        <div class="form-group s_website_form_field col-12 s_website_form_required" data-type="email" data-name="Field" data-visibility-condition="" data-visibility-between="">
                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="ow0azb08j34a">
                                    <span class="s_website_form_label_content">Your Email</span>
                                    <span class="s_website_form_mark"> *</span>
                                </label>
                                <div class="col-sm">
                                    <input type="email" class="form-control s_website_form_input" name="email_from" required="1" data-fill-with="email" id="ow0azb08j34a"/>
                                </div>
                            </div>
                        </div>
                        <div data-visibility-condition="" data-visibility-between="" class="form-group s_website_form_field col-12 s_website_form_required" data-type="char" data-name="Field">
                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="oglrig16zwv">
                                    <span class="s_website_form_label_content">Your Organization</span>
                                    <span class="s_website_form_mark">       *</span>
                                </label>
                                <div class="col-sm">
                                    <input type="text" class="form-control s_website_form_input" name="partner_name" required="1" placeholder="" id="oglrig16zwv" data-fill-with="commercial_company_name"/>
                                </div>
                            </div>
                        </div>
                        <div class="form-group s_website_form_field col-12 s_website_form_required" data-type="char" data-name="Field">
                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="orzppxhjcv7a">
                                    <span class="s_website_form_label_content">Subject</span>
                                </label>
                                <div class="col-sm">
                                    <input type="text" class="form-control s_website_form_input" name="name" required="true" id="orzppxhjcv7a" t-att-value="proposal.name" readonly="readonly"/>
                                </div>
                            </div>
                        </div>
                        <div class="form-group s_website_form_field col-12 s_website_form_required" data-type="text" data-name="Field">
                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="osgztz030wp">
                                    <span class="s_website_form_label_content">Your Question</span>
                                    <span class="s_website_form_mark"> *</span>
                                </label>
                                <div class="col-sm">
                                    <textarea class="form-control s_website_form_input" name="description" required="1" id="osgztz030wp" rows="3" data-gramm="false" wt-ignore-input="true"></textarea>
                                </div>
                            </div>
                        </div>
                        <div class="form-group s_website_form_field col-12 s_website_form_dnone" data-name="Field">
                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                <label class="col-form-label col-sm-auto s_website_form_label">
                                    <span class="s_website_form_label_content"></span>
                                </label>
                                <div class="col-sm">
                                    <input type="hidden" class="form-control s_website_form_input" name="team_id" value="1"/>
                                </div>
                            </div>
                        </div>
                        <div class="form-group s_website_form_field col-12 s_website_form_dnone" data-name="Field">
                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                <label class="col-form-label col-sm-auto s_website_form_label">
                                    <span class="s_website_form_label_content"></span>
                                </label>
                                <div class="col-sm">
                                    <input type="hidden" class="form-control s_website_form_input" name="user_id" value="6"/>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-12 s_website_form_submit" data-name="Submit Button" data-quillbot-parent="PzJbc6QQN3nGaRcmsT1Js">
                            <div style="width: 200px;" class="s_website_form_label"/>
                            <a href="#" role="button" class="btn btn-primary btn-lg s_website_form_send" data-original-title="" title="" data-gramm="false" wt-ignore-input="true" data-quillbot-element="PzJbc6QQN3nGaRcmsT1Js" aria-describedby="popover563789">Submit</a>
                            <span id="s_website_form_result"/>
                            <quillbot-extension data-element-id="PzJbc6QQN3nGaRcmsT1Js" style="position: absolute; top: 0px; left: 0px; pointer-events: none;"/>
                            <quillbot-extension-highlights data-element-id="PzJbc6QQN3nGaRcmsT1Js" data-element-type="html" style="position: absolute; top: 0px; left: 0px; pointer-events: none;"/>
                        </div>
                    </form>
                </section>
            </div>
        </t>
    </template>
</odoo>
